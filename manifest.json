{
  "name": "Cookie Eraser",
  "description": "Erase All Cookies",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "19": "icons/icon19.png",
    "48": "icons/icon48.png"
  },
  "browser_action": {
      "name": "Cookie Eraser",
      "default_icon": "icons/icon19.png",
      "default_title": "Reload",
      "default_popup": "popup.html"
  },
  "permissions": [
      "cookies","tabs",
      "http://*/*", "https://*/*"
  ]
}